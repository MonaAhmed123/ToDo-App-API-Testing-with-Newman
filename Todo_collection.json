{
	"info": {
		"_postman_id": "894618e0-c865-4ba5-9895-1f5fcb315c8e",
		"name": "Todo QaCart Project",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
		"_exporter_id": "9423880"
	},
	"item": [
		{
			"name": "Register",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"pm.environment.set(\"base_url\", \"https://todo.qacart.com\");\r",
							"const firstName =pm.variables.replaceIn('{{$randomFirstName}}')\r",
							"pm.environment.set(\"firstName\", firstName);"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"register Status code is 201\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"\r",
							"pm.test(\"first name should be correct\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"\r",
							"    const firstName= pm.environment.get(\"firstName\");\r",
							"    const token=jsonData.access_token\r",
							"    pm.environment.set(\"token\",token);\r",
							"    pm.expect(jsonData.firstName).to.eql(firstName);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"email\": \"{{$randomEmail}}\",\r\n    \"password\": \"{{$randomPasswor}}\",\r\n    \"firstName\": \"{{firstName}}\",\r\n    \"lastName\": \"{{$randomLastName}}\"\r\n}\r\n\r\n",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{base_url}}/api/v1/users/register",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"api",
						"v1",
						"users",
						"register"
					]
				}
			},
			"response": []
		},
		{
			"name": "Add Todo",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 201\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"\r",
							"var jsonData = pm.response.json();\r",
							"const taskID=jsonData._id\r",
							"\r",
							"pm.environment.set(\"taskID\",taskID);\r",
							"pm.test(\"todo name must be correct\", function () {\r",
							"    pm.expect(jsonData.item).to.eql(\"postman project\");\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "bearer",
					"bearer": [
						{
							"key": "token",
							"value": "{{token}}",
							"type": "string"
						}
					]
				},
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"item\": \"postman project with newman\",\r\n    \"isCompleted\": false\r\n}",
